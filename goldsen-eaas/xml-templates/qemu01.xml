<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<emulationEnvironment xmlns="http://bwfla.bwl.de/common/datatypes">
 <id>BIBIDCDNO</id>
 <description>
    <title>MARCTITLE</title>
 </description>
 <arch>i386</arch>
 <emulator bean="Qemu"></emulator>

<ui_options>
  <html5>
   <pointer_lock>false</pointer_lock>
   <crt></crt>
  </html5>
 </ui_options>


 <drive>
  <data>binding://main_hdd</data>
  <iface>ide</iface>
  <bus>0</bus>
  <unit>0</unit>
  <type>disk</type>
  <boot>true</boot>
  <plugged>true</plugged>
 </drive>

<drive>
  <data>binding://cd</data>
  <iface>ide</iface>
  <bus>0</bus>
  <unit>1</unit>
  <type>cdrom</type>
  <boot>false</boot>
  <filesystem>ISO</filesystem>
 </drive>

 <binding id="main_hdd">
  <url>imagearchive:EMUPATH</url>
  <access>cow</access>
 </binding>

 <binding id="cd">
  <url>imagearchive:ARTPATH.iso</url>
  <access>cow</access>
 </binding>

 <nativeConfig linebreak="[[nl]]">
 </nativeConfig>

</emulationEnvironment>
